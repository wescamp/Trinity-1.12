#. [side]: type=Primeval Nemesishi, id=Nemesis
#. [side]: type=Primeval Nemesislow, id=Nemesis
#. [side]: type=Primeval Nemesislow, id=Nemesis_
#: Trinity/scenarios/3_P01_Village.cfg:291
#: Trinity/scenarios/3_P02_Entrance.cfg:177
#: Trinity/scenarios/3_P03_Cave.cfg:227 Trinity/scenarios/3_P04_Engine.cfg:402
